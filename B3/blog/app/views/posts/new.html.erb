<%= form_for :post,url: posts_path do |f| %>
<p>
  <%= f.label :type %>(1 or 2) <br>
  <%= f.text_field :p_type%>
</p>

<p>
  <%= f.label :title %><br>
  <%= f.text_field :title%>
</p>

<p>
  <%= f.label :content %><br>
  <%= f.text_area :content%>
</p>

<p>
  <%= f.submit%>
</p>

<% end %>

<%= link_to 'Back',admins_url%><br>
